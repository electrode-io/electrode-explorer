{
  "plugins": {
    "inert": {
      "enable": true
    },
    "./server/search-api": {
      "module": "./{{env.APP_SRC_DIR}}server/search-api"
    },
    "./server/component-api": {
      "module": "./{{env.APP_SRC_DIR}}server/component-api"
    },
    "./server/poll": {
      "module": "./{{env.APP_SRC_DIR}}server/poll",
      "options": {
        "enable": true
      }
    },
    "webapp": {
      "module": "electrode-react-webapp/lib/hapi",
      "options": {
        "pageTitle": "Electrode Explorer",
        "paths": {
          "/{args*}": {
            "view": "index",
            "content": {
              "module": "./{{env.APP_SRC_DIR}}server/views/index-view"
            }
          }
        }
      }
    }
  },

  "githubApi": {
    "version": "3.0.0",
    "pathPrefix": "",
    "protocol": "https",
    "host": "api.github.com"
  },

  "ORGS": [

  ],

  "REPOS_USAGE_INCLUDE": [

  ],

  "REPOS_USAGE_EXCLUDE": [

  ],

  "MODULE_PREFIXES_INCLUDE": [

  ],

  "NPM_WAITING_TIME": 300000,

  "GHACCESS_TOKEN_NAME": "GHACCESS_TOKEN"
}
